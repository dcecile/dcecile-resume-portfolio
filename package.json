{
  "engines": {
    "node": "^17.9.0",
    "pnpm": "^7.17.0"
  },
  "dependencies": {
    "create-elm-app": "^5.22.0",
    "elm": "0.19.1-5",
    "elm-format": "^0.8.5",
    "postcss": "^8.1.0",
    "prettier": "^2.7.1",
    "puppeteer": "^19.2.2"
  },
  "devDependencies": {
    "chokidar-cli": "^3.0.0"
  },
  "scripts": {
    "clean": "rm -rf elm-stuff",
    "make-src": "elm make src/Main.elm --output /dev/null",
    "make-tests": "cd ./tests && elm make *.elm --output /dev/null",
    "test": "elm-app test",
    "validate-format": "elm-format --validate src/ tests/ && pnpm run prettier-all --list-different",
    "prettier-all": "prettier '**/*.js' '!**/build/**' '!**/elm-stuff/**'"
  }
}
